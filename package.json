{
  "name": "exif-pilot",
  "version": "1.0.2",
  "description": "Exif Pilot makes it easy to read, add, and update EXIF metadata in images with a simple and flexible API.",
  "main": "./dist/index.cjs",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "prepack": "npm run build"
  },
  "files": [
    "./dist/index.cjs",
    "./dist/index.mjs"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "keywords": [
    "exif",
    "metadata",
    "image",
    "tool",
    "image",
    "png",
    "jpg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/6f5/exif-pilot"
  },
  "author": "Adbulla Saeed <me@restoredbytes.com> (https://github.com/6f5)",
  "license": "MIT",
  "dependencies": {
    "exiftool-vendored": "28.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "rollup": "4.21.2"
  }
}
